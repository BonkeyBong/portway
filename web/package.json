{
  "name": "portway-web",
  "version": "1.0.0",
  "description": "Portway Web Client",
  "main": "src/server/app.js",
  "scripts": {
    "analyze": "NODE_ENV=production webpack --env.production --config ./config/webpack.prod.js --profile --json > stats.json && ./node_modules/webpack-bundle-analyzer/lib/bin/analyzer.js stats.json",
    "serve": "NODE_ENV=development nodemon --watch ./src/server --exec babel-node -r node_modules/dotenv/config ./src/server/app.js dotenv_config_path=./src/server/.env",
    "build": "NODE_ENV=production webpack --env.production --config ./config/webpack.prod.js && babel ./src/server -d dist/server --copy-files && babel ./src/shared -d dist/shared --copy-files",
    "start": "NODE_ENV=production node ./dist/server/app.js",
    "babel-node": "babel-node",
    "test": "jest --config ./config/jest.config.js --env=jsdom",
    "test-watch": "jest --config ./config/jest.config.js --env=jsdom --watch",
    "coverage": "jest --config ./config/jest.config.js --env=jsdom --coverage --colors",
    "puppeteer": "JEST_PUPPETEER_CONFIG=config/puppeteer.config.js jest --config ./config/jest-puppeteer.config.js --runInBand --colors",
    "preinstall": "npx npm-force-resolutions"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:BonkeyBong/portway.git"
  },
  "author": "BonkeyBong, LLC",
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "lint-staged": {
    "linters": {
      "*.{js,jsx,md,css}": [
        "pretty-quick --staged",
        "eslint src/ --fix",
        "git add"
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.52.2",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^1.0.0",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "jest-environment-node": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "kind-of": "^6.0.3",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.4.4",
    "minimist": "^1.2.5",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^1.19.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "puppeteer": "^2.1.1",
    "react": "^16.13.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-image-crop": "^8.6.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.1.2",
    "react-select": "^2.4.4",
    "react-stripe-elements": "^2.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.42.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-fix-style-only-entries": "^0.4.0",
    "webpack-hot-middleware": "^2.25.0",
    "zxcvbn": "^4.4.2"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "dependencies": {
    "@slack/webhook": "^5.0.3",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "connect-gzip-static": "^2.1.1",
    "cookie-parser": "^1.4.5",
    "cuid": "^2.1.8",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "^4.17.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.0",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "r7insight_node": "^2.0.0"
  }
}
