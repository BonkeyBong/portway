{
  "name": "project-danger-web",
  "version": "1.0.0",
  "description": "Project Danger Web Client",
  "main": "src/server/app.js",
  "scripts": {
    "serve": "NODE_ENV=development nodemon --watch ./src/server --exec babel-node -r node_modules/dotenv/config ./src/server/app.js dotenv_config_path=./src/server/.env",
    "build": "NODE_ENV=production webpack --env.production --config ./config/webpack.prod.js && babel ./src/server -d dist/server --copy-files && babel ./src/shared -d dist/shared --copy-files",
    "start": "NODE_ENV=production node ./dist/server/app.js",
    "babel-node": "babel-node",
    "test": "jest --config ./config/jest.config.js --env=jsdom",
    "test-watch": "jest --config ./config/jest.config.js --env=jsdom --watch",
    "coverage": "jest --config ./config/jest.config.js --env=jsdom --coverage --colors"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:BonkeyBong/project-danger.git"
  },
  "author": "BonkeyBong, LLC",
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "lint-staged": {
    "linters": {
      "*.{js,jsx,md,css}": [
        "pretty-quick --staged",
        "eslint src/ --fix",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.2.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "bulma": "^0.7.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-stripe-elements": "^2.0.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "simplemde": "^1.11.2",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-fix-style-only-entries": "^0.1.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "^4.16.4",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-local": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
