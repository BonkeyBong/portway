<%- include('../partials/head', { title: title, permalink: permalink, robots: 'noindex, nofollow' }); %>
<div id="application" role="application">
  <!-- Overwrittem by js/app.js -->
  <style>
    :root {
      color-scheme: light dark;
      --base-background: #fff;
      --base-color: #3b3d3e;
    }
    @media screen and (prefers-color-scheme: dark) {
      :root {
        --base-background: #3b3d3e;
        --base-color: #fff;
      }
    }
    * { box-sizing: border-box; }
    body {
      background-color: var(--base-background);
      color: var(--base-color);
      margin: 0;
      min-height: 100vh;
      overflow-x: hidden;
      padding: 0;
    }
    .masthead {
      border-top: 6px solid #f8963a;
      height: 71px;
    }
    .navbar {
      align-items: center;
      border-bottom: thin solid #e5e7e6;
      display: flex;
      height: 64px;
      justify-content: space-between;
    }
    .navbar__icon {
      background-color: #e5e7e6;
      border-radius: 50%;
      height: 40px;
      overflow: hidden;
      width: 40px;
    }
    .navbar__brand {
      align-items: center;
      display: flex;
      height: 100%;
      justify-content: center;
      width: 64px;
    }
      .navbar__brand .skeleton {
        animation-delay: 0.3s;
      }
    .navbar__user {
      align-items: center;
      border-left: thin solid #e5e7e6;
      height: 100%;
      display: flex;
      justify-content: center;
      width: 64px;
    }
      .navbar__user .skeleton {
        animation-delay: 0.5s;
      }
    .navbar__content {
      align-items: center;
      display: flex;
      flex: 1;
    }
      .navbar__content .skeleton {
        animation-delay: 0.8s;
        width: 200px;
      }
    .logo-bg {
      animation: logo 5s linear infinite;
      fill: #e8eae8;
    }
    .skeleton{
      animation: loading 5s linear infinite;
      background-repeat: no-repeat;
      background: linear-gradient(90deg, #fff, #e8eae8, #fff);
      background-size: 300% 300%;
      display: block;
      height: 100%;
      position: relative;
      width: 100%;
    }
      .skeleton.line {
        height: 16px;
      }
    @keyframes loading {
      0% { background-position: 0% 50%; }
      50% { background-position: 100% 51%; }
      100% { background-position: 0% 50%; }
    }
    .logo {
      align-items: center;
      display: flex;
      flex-direction: column;
      height: calc(100vh - 71px);
      justify-content: center;
      width: 100%;
    }
    @keyframes logo {
      0% { fill: #e8eae8; }
      50% { fill: rgba(232, 234, 232, .2); }
      100% { fill: #e8eae8; }
    }
  </style>
  <div>
    <header class="masthead" role="banner">
      <nav class="navbar" role="navigation" aria-label="main navigation">
        <div class="navbar__brand">
          <div class="navbar__icon">
            <span class="skeleton round"></span>
          </div>
        </div>
        <div class="navbar__content">
          <span class="skeleton line"></span>
        </div>
        <div class="navbar__misc"></div>
        <div class="navbar__user">
          <div class="navbar__icon">
            <span class="skeleton round"></span>
          </div>
        </div>
      </nav>
    </header>
