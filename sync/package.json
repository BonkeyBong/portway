{
  "name": "portway-sync",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "serve": "NODE_ENV=development nodemon --exec babel-node --require dotenv/config -- src/server.js",
    "build": "NODE_ENV=production babel ./src -d dist/ --copy-files",
    "start": "NODE_ENV=production node ./dist/server.js",
    "babel-node": "babel-node"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "babel-preset-env": "^1.7.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.1.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "~4.16.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.1"
  }
}
