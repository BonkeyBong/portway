{
  "name": "@portway/workers",
  "version": "1.0.0",
  "description": "Worker queue for offloading portway tasks",
  "scripts": {
    "serve": "NODE_ENV=development npx nodemon --exec npx babel-node --require dotenv/config -- src/server.js",
    "build": "NODE_ENV=production babel ./src -d dist/ --copy-files",
    "start": "NODE_ENV=production node ./dist/server.js",
    "babel-node": "babel-node",
    "test": "npm run test-u",
    "test-u": "jest 'src/.*\\.test-(u)\\.js$'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.721.0",
    "axios": "^0.21.1",
    "bee-queue": "^1.2.3",
    "cuid": "^2.1.8",
    "mime-types": "^2.1.29",
    "moment": "^2.27.0",
    "ono": "^7.1.3",
    "r7insight_node": "^2.0.2",
    "rimraf": "^3.0.2",
    "sharp": "^0.27.1",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "dotenv": "^8.2.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4"
  },
  "jest": {
    "testRegex": "src/.*\\.test-(u|a|i)\\.js$",
    "verbose": true,
    "testEnvironment": "node"
  }
}
